{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ],
  "redirects": [
    { "source": "/Admin", "destination": "/admin", "permanent": true },
    { "source": "/Blog", "destination": "/blog", "permanent": true },
    { "source": "/Evolution", "destination": "/evolution", "permanent": true },
    { "source": "/Projets", "destination": "/projets", "permanent": true },
    { "source": "/Contact", "destination": "/contact", "permanent": true },
    { "source": "/A-Propos", "destination": "/a-propos", "permanent": true },
    { "source": "/Partenariat", "destination": "/partenariat", "permanent": true },
    { "source": "/Agricapital", "destination": "/agricapital", "permanent": true },
    { "source": "/Vision", "destination": "/vision", "permanent": true },
    { "source": "/Login", "destination": "/login", "permanent": true },
    { "source": "/:path+/", "destination": "/:path+", "permanent": true }
  ],
  "rewrites": [
    { "source": "/blog/:slug", "destination": "/index.html" },
    { "source": "/admin", "destination": "/index.html" },
    { "source": "/blog", "destination": "/index.html" },
    { "source": "/evolution", "destination": "/index.html" },
    { "source": "/a-propos", "destination": "/index.html" },
    { "source": "/vision", "destination": "/index.html" },
    { "source": "/agricapital", "destination": "/index.html" },
    { "source": "/projets", "destination": "/index.html" },
    { "source": "/partenariat", "destination": "/index.html" },
    { "source": "/contact", "destination": "/index.html" },
    { "source": "/login", "destination": "/index.html" },
    { "source": "/mentions-legales", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}